{
    "name": "bachphuc/laravel-theme",
    "description": "theme manager package for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "bachphuc",
            "email": "trieuquangphuc123@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "bachphuc\\LaravelTheme\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "bachphuc\\LaravelTheme\\Providers\\PackageServiceProvider"
            ],
            "aliases": {
                "LaravelTheme" : "bachphuc\\LaravelTheme\\Facades\\LaravelThemeFacade",
                "Theme" : "bachphuc\\LaravelTheme\\Facades\\LaravelThemeFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
